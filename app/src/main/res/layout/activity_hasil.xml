<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".HasilActivity">

    <TextView
        android:id="@+id/tampilNama"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/tampil_nama" />

    <TextView
        android:id="@+id/tampilJenisKelamin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/tampil_jenis_kelamin" />

    <TextView
        android:id="@+id/tampilSetuju"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/tampil_setuju" />

</LinearLayout>